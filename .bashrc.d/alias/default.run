# ls
ls:ls -Fh --color=auto
ll:ls -l
la:ls -la
lrt:ls -lrt

# crontab
crl:crontab -l
cre:crontab -e
crg:crontab -l | grep -v '^#|^$' | grep -i
crs:crontab -l | grep -v '^#|^$' | sort -n -k 2 -k 1

# rm
rm:rm -i

# du
du:du -skh
